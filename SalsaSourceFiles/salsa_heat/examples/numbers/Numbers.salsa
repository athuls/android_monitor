module examples.numbers;


behavior Numbers {
   
   
    void Dummy(){

	Number number = new Number(0);

	Adder adder = new Adder(number);
	Printer printer = new Printer(number);

	for (int i = 0 ; i<10; i++){
	    adder<-go();
	    printer<-go();
	}
    }

    void act(String args[]){
	
    Dummy();
   
	standardOutput<-println();
    }
}
